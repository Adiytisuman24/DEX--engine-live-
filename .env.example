# Execution Mode: 'mock' or 'devnet'
EXECUTION_MODE=mock

# Solana Configuration (Required for devnet mode)
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_CLUSTER=devnet

# Wallet Private Key (Base58 encoded - NEVER commit real keys!)
# Generate with: solana-keygen new
# For testing only - use a throwaway devnet wallet
WALLET_PRIVATE_KEY=your_base58_private_key_here

# Database
POSTGRES_USER=admin
POSTGRES_PASSWORD=password
POSTGRES_DB=dex_engine

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# API
PORT=3000

# Safety Limits (Devnet)
MAX_TRADE_AMOUNT_SOL=0.1
MAX_SLIPPAGE_BPS=500
